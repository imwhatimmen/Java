public class Mascota{
    //atributos
    String nombre;
    String tipo;//si es un perro, gato,...
    int fecha[3];//dia, mes, año
    char sexo;
    //Constructor
    public Mascota(){
        this.nombre="nombre desconocido";
        this.tipo="indeterminado";
    
    }
    
    //Métodos
    String devuelveNombre(){
        return(nombre);
    }
    String devuelvetipo(){
        return(tipo);
    }
    char devuelveSexo(){
        return(sexo);
    }
    void cambiarNombre(String nuevoNombre){
        nombre=nuevoNombre;
    }
}
    public class principal(){
        public static void main(String args[]){
            Mascota miPerritoPopo;
            miPerritoPopo=new Mascota();
        }
}
